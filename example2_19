using System;
class IntegerConstantApp
{
    public static void Main()
    {
        int x = 9, y = 3;
        Console.WriteLine(x + " & " + y + " = " + (x & y));
        Console.WriteLine(x + "   " + y + " = " + (x | y));
        Console.WriteLine(x + " ^ " + y + " = " + (x ^ y));
        Console.WriteLine("~10 = " + (~10));
    }
}
