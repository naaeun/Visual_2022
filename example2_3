using System;
class IntegerConstantApp
{
    public static void Main()
    {
        float f1 = 1.414F, f2 = 0.1414e01f;
        double d = 0.1414E1;
        Console.WriteLine("f1 = {0}, f2 = {1}, d = {2}", f1, f2, d);
        if (f1==f2)
            Console.WriteLine("Yes");
        else
            Console.WriteLine("No");
    }
}
