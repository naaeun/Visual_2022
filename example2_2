using System;
class IntegerConstantApp
{
    public static void Main()
    {
        int i = 255, h = 0xff;
        long l = 0xffL; //longí˜•
        Console.WriteLine("i = {0}, h = {1}", i, h);
        if (h == 1)
            Console.WriteLine("Yes");
        else
            Console.WriteLine("No");
    }
}
