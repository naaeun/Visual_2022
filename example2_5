using System;
class IntegerConstantApp
{
    public static void Main()
    {
        char c = 'A';
        int i;
        i = c + 1;

        Console.WriteLine("c = " + c + "\nnext c = " + (char)i);
        Console.WriteLine("\"I am a string.\"");
        Console.WriteLine(@"""I am a string.""");
    }
}
